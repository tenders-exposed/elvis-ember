<td class="name">
  {{#link-to "network.show.details.contract" route nodeId row.lot.tender.id}}
    {{fa-icon 'file-text' size=1}}
    {{row.lot.tender.title}}
    {{#if row.lot.title}}
      - {{row.lot.title}}
    {{/if}}
  {{/link-to}}
</td>
<td class="buyer">
  <span class="figure-bidder small"></span>
  {{#each row.bidders as |bidder|}}
    {{#link-to "network.show.details.show" "bidders" bidder.nodeID}}
      {{bidder.name}}
    {{/link-to}}
    <br>
  {{/each}}
  <br>
</td>
<td class="bids">{{row.lot.bidsCount}}</td>
<td class="award">{{format-amount row.value '0,0.00'}} â‚¬</td>